{
  "variables": {
    "sakura_token": "{{env `SAKURACLOUD_ACCESS_TOKEN`}}",
    "sakura_token_secret": "{{env `SAKURACLOUD_ACCESS_TOKEN_SECRET`}}",
    "sakura_zone": "{{env `SAKURACLOUD_ZONE`}}",
    "debian_version": "8.7.1",
    "sha1_checksum": "81bc2c68de1e0581d19e4c559be913968cf8bb5a"
  },
  "builders": [
    {
      "type": "sakuracloud",
      "access_token": "{{user `sakura_token`}}",
      "access_token_secret": "{{user `sakura_token_secret`}}",
      "zone": "{{user `sakura_zone`}}",
      "archive_name": "jessie",
      "os_type": "iso",
      "iso_url": "http://cdimage.debian.or.jp/{{user `debian_version`}}/amd64/iso-cd/debian-{{user `debian_version`}}-amd64-netinst.iso",
      "iso_checksum": "{{user `sha1_checksum`}}",
      "iso_checksum_type": "sha1",
      "boot_wait": "5s",
      "boot_command": [
        "<esc><wait>",
        "auto <wait>",
        "url^http<leftShiftOn>;<leftShiftOff>//wate.github.io/installer/debian/8/preseed.cfg <wait>",
        "hostname^jessie <wait>",
        "domain^vagrantup.com <wait>",
        "frontend^noninteractive <wait>",
        "<enter><wait>"
      ],
      "ssh_password": "vagrant",
      "ssh_username": "vagrant"
    }
  ],
  "provisioners": [
    {
        "type": "shell",
        "inline": [
            "echo hoge"
        ]
    }
  ]
}
